%
% Complex meter supports multiple denominators
% numerators may be grouped over a single denominator using the 'group' parameter
%
{
[
  \meter<"2+1+3/4", size=1> a/8 a c2/4 a1/8 e2 e/4 d c
]
,
[
  \meter<"4+2/16 + 2/8 + 3 + 2/4", size=1, group="off"> 
	a/8 a c2/4 a1/8 e2 e/4 d c
]
,
[
  \meter<"4+2/16 + 2/8 + 3 + 2/4", group="on"> 
	a/8 a c2/4 a1/8 e2 e/4 d c
]
}